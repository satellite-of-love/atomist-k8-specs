{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "summon-internet-kitties",
	"annotations": {
	  "atomist.dnsmode" : "vanity",
          "atomist.elb-service-name" : "kong",
          "atomist.upstream_url" : "http://summon-internet-kitties:8080",
          "atomist.vanity-name" : "survey.atomist.com",
          "atomist.request_path": "/auckland"
	}
    },
    "spec": {
        "selector": {
            "app": "summon-internet-kitties"
        },
        "ports": [
            {
                "name": "summon-internet-kitties",
                "protocol": "TCP",
                "port": 8080,
                "targetPort": 8080
            }
        ]
    }
}
 
