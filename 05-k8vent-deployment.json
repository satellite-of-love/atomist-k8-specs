{
  "apiVersion" : "extensions/v1beta1",
  "kind" : "Deployment",
  "metadata" : {
    "name" : "k8vent",
    "labels" : {
      "app" : "k8vent"
    }
  },
  "spec" : {
    "replicas" : 1,
    "selector" : {
      "matchLabels" : {
        "app" : "k8vent"
      }
    },
    "template" : {
      "metadata" : {
        "labels" : {
          "app" : "k8vent",
          "version" : "0"
        },
        "annotations" : {
          "atomist.updater" : "{sforzando-docker-dockerv2-local.artifactoryonline.com/k8vent atomisthq/k8vent}"
        }
      },
      "spec" : {
        "containers" : [ {
          "name" : "k8vent",
          "image" : "sforzando-docker-dockerv2-local.artifactoryonline.com/k8vent:0.4.2",
          "resources" : {
            "limits" : {
              "cpu" : 0.1,
              "memory" : "100Mi"
            },
            "requests" : {
              "cpu" : 0.1,
              "memory" : "50Mi"
            }
          }
        } ],
        "dnsPolicy" : "ClusterFirst",
        "restartPolicy" : "Always",
        "imagePullSecrets" : [ {
          "name" : "atomistregistrykey"
        } ]
      }
    },
    "strategy" : {
      "type" : "RollingUpdate",
      "rollingUpdate" : {
        "maxUnavailable" : 0,
        "maxSurge" : 1
      }
    }
  }
}